<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="flattenThemeCommand" xsi:type="object">MeetMagentoPL\Falkowskifier\Console\Command\FlattenThemeCommand</item>
            </argument>
        </arguments>
    </type>
    
    <preference for="\MeetMagentoPL\Falkowskifier\Model\ThemeFileCollectorInterface"
                type="\MeetMagentoPL\Falkowskifier\Model\ThemeFileCollector"/>
</config>
